            <Grid x:Name="buttonLayout" RenderTransformOrigin="0.5, 0.5">
                <Rectangle x:Name="buttonSurface" RadiusX="5" RadiusY="5" >
                    <Rectangle.Stroke>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
